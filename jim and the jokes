#include <stdio.h>
#include <stdlib.h>

typedef struct {
    int month;
    int day;
} Event;

int is_valid_in_base(int day, int base) {
    int num = day;
    while (num > 0) {
        if (num % 10 >= base) {
            return 0; // Invalid digit for the base
        }
        num /= 10;
    }
    return 1;
}

int convert_to_base(int day, int base) {
    int result = 0;
    int multiplier = 1;
    while (day > 0) {
        result += (day % 10) * multiplier;
        multiplier *= base;
        day /= 10;
    }
    return result;
}

int main() {
    int n;
    scanf("%d", &n);

    Event events[n];
    for (int i = 0; i < n; i++) {
        scanf("%d %d", &events[i].month, &events[i].day);
    }

    int joke_count = 0;
    for (int i = 0; i < n; i++) {
        for (int j = i + 1; j < n; j++) {
            if (events[i].month != events[j].month && events[i].day != events[j].day) {
                if (is_valid_in_base(events[i].day, events[j].month) &&
                    is_valid_in_base(events[j].day, events[i].month)) {
                    int decimal1 = convert_to_base(events[i].day, events[j].month);
                    int decimal2 = convert_to_base(events[j].day, events[i].month);
                    if (decimal1 == decimal2) {
                        joke_count++;
                    }
                }
            }
        }
    }

    printf("%d\n", joke_count);
    return 0;
}
